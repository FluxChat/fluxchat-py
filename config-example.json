{
	"address": "${PYCHAT_ADDRESS}",
	"port": ${PYCHAT_PORT},
	"contact": "${PYCHAT_CONTACT}",
	"id": "${PYCHAT_ID}",
	"data_dir": "${PYCHAT_DATA_DIR}",
	"address_book": {
		"max_clients": 20,
		"client_retention_time": 24
	},
	"discovery": {
		"enabled": false,
		"port": 26000
	},
	"bootstrap": "default",
	"ipc": {
		"enabled": true,
		"address": "127.0.0.1",
		"port": 26001
	},
	"mail": {
		"message_retention_time": 24
	},
	"log": {
		"level": "warning",
		"file": "${PYCHAT_LOG_FILE}"
	}
}
